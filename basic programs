AREA OF A SQUARE

#include <stdio.h>
// Area of the square
int main() {
    
    int a;
    printf("Enter a:");
    scanf("%d",&a);
    int sq = a*a;
    printf("The area of the square is:%d",sq);

    return 0;
}

----------------------------------------------------------------------

AREA OF CIRCLE

#include <stdio.h>

int main() {
    float r;
    printf("Enter radius of the circle:");
    scanf("%f",&r);
    printf("The area of the circle is:%f",3.14*r*r);

    return 0;
}


-----------------------------------------------------------------------

TO CHECK IF A NUMBER IS DIVISIBLE BY 2

#include <stdio.h>

int main() {
    int a;
    printf("I will check if a number is divisible or not\n");
    printf("Enter your number:");
    scanf("%d",&a);
    if (a%2 == 0){
        printf("This number is divisible by 2");
    }
    else if (a%2 != 0){
        printf("This number is not divisible by 2");
    }
    return 0;
}

-----------------------------------------------------------------------

TO CHECK THE AVERAGE OF THREE NUMBERS

#include <stdio.h>

int main() {
    int a,b,c;
    printf("Enter three numbers:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    printf("The average of the numbers is %d",(a+b+c)/3);
    return 0;
}


----------------------------------------------------------------------

RANKING SYSTEM

#include <stdio.h>
int main(){
    int marks;
    printf("Enter your marrks:");
    scanf("%d",&marks);
    if(marks <= 30){
        printf("C rank");
    } else if (marks > 30 && marks < 70){
        printf("B rank");
    } else if (marks > 70 && marks < 90){
        printf("A rank");
    } else if (marks > 90 && marks <=100){
        printf("A+ rank");
    } else {
        printf("Invalid marks");
    }
    return 0;
}

----------------------------------------------------------------------

CHARACTER INDENTIFIER

#include <stdio.h>
int main(){
    char ch;
    printf("Enter a character:");
    scanf("%c",&ch);
    if (ch >= 'A' && ch <= 'Z'){
        printf("UPPER CASE");
    } else if (ch >= 'a' && ch <= 'z'){
        printf("LOWER CASE");
    } else{
        printf("invalid character");
    }
    return 0;
}

------------------------------------------------------------------------

SUM OF (N) NATURAL NUMBERS

#include <stdio.h>
int main(){
    int a;
    printf("Enter a number:");
    scanf("%d",&a);
    int sum =0;
    for (int i = 1; i<=a ; i++){
        sum=sum+i;
    }
    printf("%d \n",sum);
    return 0;
}

------------------------------------------------------------------------

TABLE OF ANY NUMBER YOU WANT

#include <stdio.h>
int main(){
    int a;
    printf("Enter a number:");
    scanf("%d",&a);
    int mul = 1;
    for (int i = 1;i <=10;i++){
        mul = a * i;
        printf("%d X %d = %d\n",a,i,mul);
    }
    return 0;
}

------------------------------------------------------------------------

KEEPS TAKING INPUT UNTIL ODD NUMBER IS INPUT

#include <stdio.h>
int main(){
    for(int i=0; ; i++){
        int a;
        printf("Enter number:");
        scanf("%d",&a);
        if(a % 2 != 0){
            break;
        }
    }
    printf("EnD");
    return 0;
}

------------------------------------------------------------------------

FACTORIAL OF ANY NUMBER

#include <stdio.h>

int main() {
    int a;
    printf("Enter number you want the factorial of:");
    scanf("%d",&a);
    int mul = 1;
    for (int i = a; i >= 1;i--){
        mul = mul * i;
    }
    printf("%d",mul);
    return 0;
}

2nd APPROACH

#include <stdio.h>
int main()
{
    int a,b,fact=1;
    printf("Enter a number you want factorial of:");
    scanf("%d",&a);
    for(int i = 1;i<=a;i++){
        fact = fact * i;
    }
    printf("Factorial = %d",fact);
    return 0;
}

------------------------------------------------------------------------

REVERSE TABLE

#include <stdio.h>

int main() {
    int a;
    printf("Enter number you want the table of:");
    scanf("%d",&a);
    for (int i = 10; i >= 1;i--){
        int mul = a * i;
        printf("%d X %d = %d\n",a,i,mul);
    }
    
    return 0;
}

